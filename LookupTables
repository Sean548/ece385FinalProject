module bigBushTable (
	input logic clk,
	input logic [1:0] in,
	input logic [24:0] out
);

always_ff @ (negedge clk)
	case (in)
	2'b00: out <= 24'h0ff0e4;
	2'b01: out <= 24'h000444;
	2'b10: out <= 24'h0a4bc0;
	2'b11: out <= 24'h1c8414;
	endcase
endmodule

module bigCloudTable (
	input logic clk,
	input logic [1:0] in,
	input logic [24:0] out
);

always_ff @ (negedge clk)
	case (in)
	2'b00: out <= 24'h0ff0e4;
	2'b01: out <= 24'h000444;
	2'b10: out <= 24'hffffff;
	2'b11: out <= 24'h0094ff;
	endcase
endmodule

