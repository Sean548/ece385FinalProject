module blueCoinsTable (
	input logic clk,
	input logic [1:0] in,
	input logic [24:0] out
);

always_ff @ (negedge clk)
	case (in)
	3'b000: out <= 24'h0ff0e4;
	3'b001: out <= 24'he69c21;
	3'b010: out <= 24'h007b8c;
	3'b011: out <= 24'h09c4a0;
	3'b100: out <= 24'b052210;
	endcase
endmodule

