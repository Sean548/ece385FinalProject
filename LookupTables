module blockBreakTable (
	input logic clk,
	input logic [1:0] in,
	input logic [24:0] out
);

always_ff @ (negedge clk)
	case (in)
	2'b00: out <= 24'h0ff0e4;
	2'b01: out <= 24'hffcec6;
	2'b10: out <= 24'h000000;
	2'b11: out <= 24'h09c4a0;
	endcase
endmodule

